{
  "name": "with-react-native-web",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .gitignore"
  },
  "devDependencies": {
    "prettier": "^3.1.1",
    "turbo": "^2.2.3"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "yarn@4.5.1"
}
